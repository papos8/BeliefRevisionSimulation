<Field>
    canvas:
        #Left line
        Rectangle:
            pos: 0, 0
            size: 5, self.height
    
        #Right line
        Rectangle:
            pos: 0, self.y
            size: 5, self.height
    
        #Bottom line
        Rectangle:
            pos: self.end, 0
            size: self.width, 5
        
    Label:
        font_size: 70
        center_x: root.width / 4
        top: root.top - 50
        text: "A"

    Label:
        font_size: 70  
        center_x: root.width * 3 / 4
        top: root.top - 50
        text: "B"